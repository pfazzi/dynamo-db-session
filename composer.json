{
    "name": "pfazzi/dynamo-db-session",
    "description": "A DynamoDb adapter for Symfony session storage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "pfazzi",
            "email": "patrick91@live.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pfazzi\\Session\\DynamoDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pfazzi\\Session\\Tests\\DynamoDb\\": "tests"
        }
    },
    "require": {
        "php": "^7.4|^8",
        "symfony/http-foundation": "^5.2",
        "aws/aws-sdk-php": "^3.171",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3",
        "squizlabs/php_codesniffer": "^3.5",
        "doctrine/coding-standard": "^8.2"
    }
}
